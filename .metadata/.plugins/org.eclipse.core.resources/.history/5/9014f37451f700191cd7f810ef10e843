package com.example;

public class Welcome {

	public static void main(String[] args) {
		greetingStudent1();
		greetingStudent2();
	

	}

	private static void greetingStudent1(){
		System.out.println("Welcome students1");
		System.out.println("Welcome students1");
		System.out.println("Welcome students1");
	}
	
	private static void greetingStudent2(){
		System.out.println("Welcome students2");
		System.out.println("Welcome students2");
		System.out.println("Welcome students2");
	}
	
}
